g1 <- highchart() %>% 
  hc_chart(type = "area") %>% 
  hc_title(text = "Evoluci칩n del voto del Chavismo/Madurismo") %>%
  hc_subtitle(text = "20 a침os, 7 procesos electorales") %>% 
  hc_xAxis(categories = as.character(a$Elecci칩n) ,
           tickmarkPlacement = "on",
           title = list(enabled = FALSE)) %>% 
  hc_tooltip(pointFormat = "<span style=\"color:{series.color}\">{series.name}</span>:
             <b>{point.percentage:.1f}%</b> ({point.y:,.0f} votos)<br/>",
             shared = TRUE) %>% 
  hc_plotOptions(area = list(
    stacking = "percent",
    lineColor = "#ffffff",
    lineWidth = 1,
    marker = list(
      lineWidth = 1,
      lineColor = "#ffffff"
    ))
  ) %>% 
  hc_add_series(name = "Oposici칩n", data = a$Nulos  %>% 
  hc_add_series(name = "Chavismo/Madurismo", data = a$Emitidos)  %>%
  hc_add_theme(hc_theme_smpl())
